{% extends 'Layout/user_layout.html.twig' %}




{% block stylesheets %}
<style>
/*NAVIGATION RAPIDE - ICONS*/

.container-all-icon{
  display: flex;
  flex-direction: row;
  justify-content: center;
  

}

.container-icon svg{
	height : 50px;
}
.container-icon span{
	margin-top : 10px;

}
.container-icon:hover span{
	-webkit-transition: color 0.1s ease;
	-moz-transition: color 0.1s ease;
	-ms-transition: color 0.1s ease;
	-o-transition: color 0.1s ease;
	transition: color 0.1s ease;
  color : #FFF;
  


}
.container-icon:hover{
  cursor: pointer;
  -webkit-transition: background-color 0.1s ease;
	-moz-transition: background-color 0.1s ease;
	-ms-transition: background-color 0.1s ease;
	-o-transition: background-color 0.1s ease;
	transition: background-color 0.5s ease;
	background-color : #FFA500;

}

.container-icon:hover svg{
	-webkit-transition: fill 0.1s ease;
	-moz-transition: fill 0.1s ease;
	-ms-transition: fill 0.1s ease;
	-o-transition: fill 0.1s ease;
	transition: fill 0.1s ease;
	fill : #FFF;
}

.container-navigation-rapide{
  display: flex;
    flex-direction: row;
    justify-content: center;
    padding-bottom : 30px;
  
  }
  
  .container-navigation-rapide a {
    text-decoration: none;
    color:#000;
    }
  .container-icon{
  
    flex: 1 0 0;
    display : flex;
    flex-direction: column;
    align-items: center;
    margin : 0 30px 0 30px ;
    padding : 10px 30px 0 30px;
  
    border-radius: 10px 10px 10px 10px;
  -moz-border-radius: 10px 10px 10px 10px;
  -webkit-border-radius: 10px 10px 10px 10px;
  }
/*AGENDA*/
.titreCategorie{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.titreCategorie a {
  margin : 15px;
  padding-top : 15px;
  font-size:  10px;
  line-height: 10px;
  text-decoration: none;
  color:#000;
}
.evenement{
  display: flex;
  flex-direction: row;
}
.date{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin : 5px 15px 5px 15px;

  flex: 0 0 80px;
  background-color: #FFA500;
  height: 80px;
  
  max-width: 80px;

  -moz-box-shadow: rgba(0, 0, 0, 0.19) 0 0 8px 0;
  -webkit-box-shadow: rgba(0, 0, 0, 0.19) 0 0 8px 0;
  box-shadow: rgba(0, 0, 0, 0.19) 0 0 8px 0;
  -moz-border-radius: 4px;
  -webkit-border-radius: 4px;
  border-radius: 4px;
}
.date .chiffre {
  margin: 0px;
  margin-top: 10px;
  font-size: 60px;
  
}
.date .mois {
  margin: 0px;
  margin-top: 10px;
}

.contenu{
  flex: 7 0 0px;
  height: 80px;
  margin : 5px 15px 5px 0;
  
}
.inverse .contenu {
  margin : 5px 0px 5px 15px;
  text-align: right;
}

.contenu .titre{
  margin: 0px;
  height : 20px;
  line-Height: 20px;
  font-weight: bold;
 
}
.contenu .description{
  margin: 0px;
  line-Height: 20px;
  height : 50px;
  font-size:  15px;
}
.contenu .voirplus{
  margin: 0px;
  font-size: 10px;
  line-Height: 10px;
}



.agenda h2{
  margin : 15px;
  padding : 0px;
}



/*SLIDER*/
 .changerImage{
  margin-top: 10px;
  position : absolute;
}

.changerImage svg:nth-child(1){
  transform: rotate(180deg);

}
.changerImage svg{
  fill :lightgrey;
  cursor: pointer;
}

.container {
	display: flex;
  flex-direction: row;
	margin-top : 30px;
}
.content{
  flex: 5 0 0px;
}
.aside{
  flex: 3 0 0px;
	padding-top : 20px;
}
.ug-tile-navigation-wrapper{
	top: 176px !important;
}


</style>


{% endblock %}

