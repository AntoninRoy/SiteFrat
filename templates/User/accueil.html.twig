{% extends 'Layout/user_layout.html.twig' %}




{% block stylesheets %}
<style>
/*NAVIGATION RAPIDE - ICONS*/

.container-all-icon{
  display: flex;
  flex-direction: row;
  justify-content: center;
  margin-bottom : 60px;
margin-top : 30px;
}

.container-icon svg{
	height : 50px;
}
.container-icon span{
	margin-top : 10px;

}
.container-icon:hover span{
	-webkit-transition: color 0.1s ease;
	-moz-transition: color 0.1s ease;
	-ms-transition: color 0.1s ease;
	-o-transition: color 0.1s ease;
	transition: color 0.1s ease;
  color : #FFF;
  


}
.container-icon:hover{
  cursor: pointer;
  -webkit-transition: background-color 0.1s ease;
	-moz-transition: background-color 0.1s ease;
	-ms-transition: background-color 0.1s ease;
	-o-transition: background-color 0.1s ease;
	transition: background-color 0.5s ease;
	background-color : #ee8600;

}

.container-icon:hover svg{
	-webkit-transition: fill 0.1s ease;
	-moz-transition: fill 0.1s ease;
	-ms-transition: fill 0.1s ease;
	-o-transition: fill 0.1s ease;
	transition: fill 0.1s ease;
	fill : #FFF;
}

.container-navigation-rapide{
  display: flex;
    flex-direction: row;
    justify-content: center;
    padding-bottom : 30px;
  
  }
  
  .container-navigation-rapide a {
    text-decoration: none;
    color:#000;
    }
  .container-icon{
  
    flex: 1 0 0;
    display : flex;
    flex-direction: column;
    align-items: center;
    margin : 0 30px 0 30px ;
    padding : 10px 45px 0 45px;
  
    border-radius: 10px 10px 10px 10px;
  -moz-border-radius: 10px 10px 10px 10px;
  -webkit-border-radius: 10px 10px 10px 10px;
  }




.partie p {
	padding-left : 30px;
	padding-right : 30px;
	padding-bottom : 30px;
}
.start{

	background-repeat: no-repeat;
    background-size: cover;
    background-position: center;
    background-color: rgba(255, 255, 255, 0.15);
	

}
.start h2 {
	padding-top : 200px;
	padding-bottom : 200px;
	color  : #ee8600;
	font-size : 100px;
	letter-spacing: 20px;
}
hr {
	height: 1px; color: #839D2D; background-color: #000; width: 50%; border: none;
	margin-top : 120px;
	margin-bottom : 120px;
}
.blanc {
	padding-top : 30px;
	padding-bottom : 30px;
}

/*AGENDA*/
.titreCategorie{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.titreCategorie a {
  margin : 15px;
  padding-top : 15px;
  font-size:  10px;
  line-height: 10px;
  text-decoration: none;
  color:#000;
}

.evenements{
	height:100%;
	  display: flex;
  flex-direction: column;
  justify-content: space-around;

}
.evenement{
  display: flex;
  flex-direction: row;
}
.date{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin : 5px 15px 5px 15px;

  flex: 0 0 60px;
  background-color: #FFA500;
  height: 60px;
  
  max-width: 60px;

  -moz-box-shadow: rgba(0, 0, 0, 0.19) 0 0 8px 0;
  -webkit-box-shadow: rgba(0, 0, 0, 0.19) 0 0 8px 0;
  box-shadow: rgba(0, 0, 0, 0.19) 0 0 8px 0;
  -moz-border-radius: 4px;
  -webkit-border-radius: 4px;
  border-radius: 4px;
}
.date .chiffre {
  margin: 0px;
  margin-top: 7px;
  font-size: 45px;
  
}
.date .mois {
margin-top : -2px;
font-size: 19px;
}

.contenu{
  flex: 7 0 0px;
	display : flex;
	align-items : center;
  margin : 5px 15px 5px 0;
  
}


.contenu .titre{
  margin: 0px;
  height : 20px;
  line-Height: 20px;
  font-weight: bold;
 
}

.agenda h2{
  margin : 15px;
  padding : 0px;
 
}
.agenda{
	 height : 100%
}
/*main + aside*/

.container {
	display: flex;
  flex-direction: row;


  
  /*padding : 0 120px 0 120px;*/
}
.content{
  flex: 6 0 0px;
}
.aside{
  flex: 3 0 0px;
}
.slider{
	max-height : 400px;

}
.container{

}
</style>


{% endblock %}

